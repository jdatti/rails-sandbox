<%= form_tag("/search", :method => "get") do %>
  <div class="field">
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
  </div>
<% end %>

<div class="">
  <table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>Title</th>
    <th>Body</th>
  </tr>

<% @songs.each do |song| %>
  <tr>
    <td><%= song.title %></td>
    <td><%= song.body %></td>
  </tr>
<% end %>
</table>
</div>